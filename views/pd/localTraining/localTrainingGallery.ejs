<%- include('../../includes/header')-%>
        <div id="wrapper" class="wrapper" >
            <%- include('../../includes/topbar')-%>
            <div class="page-wrap">
                <%- include('../../includes/sidebar')-%>
                <div class="main-content">
                    <div class="container-fluid">
                        <div class="row clearfix">
                            <div class="col-md-10">
                                <h1 class="text-center"><%= title %></h1>
                            </div>
                        </div>

                        <div class="panel-body" >
                            
                        </div>

                        <div class="container gallery-container">
                            <form action="/pd/localTrainingGalleryPost" id="FieldTrialForm" class="form-horizontal" method="post"  role="form" enctype="multipart/form-data">
                                <div class="form-group">
                                    <label >নতুন ছবি আপ্লোড করুন</label>
                                    <input type="file" id="filename" class="form-control" name="localTraining" required>
                                </div>
                                            
                                            
                         <div class="form-group">
                                    <!-- Button -->                                        
                          <div class="col-md-offset-3 col-md-3">
                            <input type="submit" name="submit" value="আপ্লোড" class="btn btn-primary">   
                        </div>

                            
                            <div class="tz-gallery">
                                <div class="row">
                                    <% records.forEach(function(row,index){ %>
                                    <div class="col-sm-12 col-md-4">
                                        <a class="lightbox" href="<%= row.image %>">
                                            <img class="chobi" src="<%= row.image %>">
                                        </a>
                                    </div>
                                    <% }) %>
                                    
                                    

                                </div>
                        
                            </div>
                        
                        </div>
                        
                        
                    </div>
                </div>
            </div>
        </div>


   
<%- include('../../includes/footer')-%>
<script src="https://cdnjs.cloudflare.com/ajax/libs/baguettebox.js/1.8.1/baguetteBox.min.js"></script>
                        <script>
                            baguetteBox.run('.tz-gallery');
                        </script>