<%if (records.length >0){ %>
    <table class="table table-bordered table-striped" id="tblexportData" cellspacing="0" cellpadding="0">
    <thead>
    <tr>
        <th>ক্রমিক নং</th>
        <th>উপজেলার নাম</th>
        <th>কৃষকের নাম ও পিতা/স্বামীর নাম</th>
        <th>গ্রাম/ব্লক</th>
        <th>মোবাইল নম্বর</th>
        <th>যে ফসল আবাদ করে পুরস্কৃত হয়েছে তার নাম ও প্রযুক্তিসমূহ</th>
        <th>মন্তব্য</th>
    </tr>  
    </thead>
    <tbody>
        <% records.forEach(function(row,index){ %>
            <tr>
                <td><%= index+1 %></td>
                <td><%= row.upazilla %></td>
                <td><%= row.name %></td>
                <td><%= row.village %></td>
                <td><%= row.mobile%></td>
                <td><%= row.crop %></td>
                <td><%= row.comment %></td>
                
            </tr>
        <% }) %>
    </tbody>
</table>
<% }else{ %>
    <table class="table table-bordered table-striped" id="tblCustomers" cellspacing="0" cellpadding="0">
        <thead>
        <tr>
            <th>ক্রমিক নং</th>
        <th>উপজেলার নাম</th>
        <th>কৃষকের নাম ও পিতা/স্বামীর নাম</th>
        <th>গ্রাম/ব্লক</th>
        <th>মোবাইল নম্বর</th>
        <th>যে ফসল আবাদ করে পুরস্কৃত হয়েছে তার নাম ও প্রযুক্তিসমূহ</th>

        <th>মন্তব্য</th>
        </tr>
        </thead>
        <tbody>
        <% records.forEach(function(row){ %>
            <tr>
                <td colspan="10">No data Found</td>
            </tr>
        <% }) %>
        </tbody>
    </table>
<% } %>