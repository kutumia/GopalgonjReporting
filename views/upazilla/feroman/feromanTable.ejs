<%if (records.length >0){ %>
    <table class="table table-bordered table-striped" id="tblexportData" cellspacing="0" cellpadding="0">
    <thead>
    <tr>
        <th>ক্রমিক নং</th>
        <th>উপজেলার নাম</th>
        <th>কৃষকের নাম ও পিতা/স্বামীর নাম</th>
        <th>গ্রাম/ব্লক</th>
        <th>মোবাইল নম্বর</th>
        <th>ফসলের নাম</th>
        <th>লিউরসহ বিতরণকৃত ফেরোমন ফাঁদের সংখ্যা</th>
        <th>উপজেলা কৃষি অফিসারের মন্তব্য</th>
        <th>জেলা পরিচালকের মন্তব্য</th>
        <th>প্রজেক্ট পরিচালকের মন্তব্য</th>
        <th>পরিবর্তন/মুছুন</th>
    </tr>  
    </thead>
    <tbody>
        <% records.forEach(function(row,index){ %>
            <tr>
                <td><%= index+1 %></td>
                <td><%= row.upazilla %></td>
                <td><%= row.name %></td>
                <td><%= row.village %></td>
                <td><%= row.mobile%></td>
                <td><%= row.crop %></td>
                <td><%= row.feroman %></td>
                <td><%= row.comment %></td>
                <td><%= row.ddComment %></td>
                <td><%= row.pdComment %></td>
                <td><a class="btn btn-primary btn-xs" href="/upazilla/feromanEdit/<%= row.id %>"><i class="fa fa-edit"></i></a>
                    <a class="btn btn-danger btn-xs" href="/upazilla/feromanDelete/<%= row.id %>"><i class="fa fa-trash"></i></a>
                </td>
            </tr>
        <% }) %>
    </tbody>
</table>
<% }else{ %>
    <table class="table table-bordered table-striped" id="tblCustomers" cellspacing="0" cellpadding="0">
        <thead>
        <tr>
            <th>ক্রমিক নং</th>
        <th>উপজেলার নাম</th>
        <th>কৃষকের নাম ও পিতা/স্বামীর নাম</th>
        <th>গ্রাম/ব্লক</th>
        <th>মোবাইল নম্বর</th>
        <th>ফসলের নাম</th>
        <th>লিউরসহ বিতরণকৃত ফেরোমন ফাঁদের সংখ্যা</th>
        <th>উপজেলা কৃষি অফিসারের মন্তব্য</th>
        <th>জেলা পরিচালকের মন্তব্য</th>
        <th>প্রজেক্ট পরিচালকের মন্তব্য</th>
        </tr>
        </thead>
        <tbody>
        <% records.forEach(function(row){ %>
            <tr>
                <td colspan="10">No data Found</td>
            </tr>
        <% }) %>
        </tbody>
    </table>
<% } %>